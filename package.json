{
  "name": "svelte-todomvc",
  "version": "1.0.0",
  "description": "TodoMVC implemented in Svelte",
  "scripts": {
    "postinstall": "dotnet tool restore",
    "build": "dotnet fable src/TodoMVC --run webpack --mode production",
    "start": "dotnet fable watch src/TodoMVC --exclude Fable.Svelte --run webpack serve --content-base public"
  },
  "files": [
    "index.html",
    "public"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sveltejs/svelte-todomvc.git"
  },
  "keywords": [
    "svelte",
    "todomvc"
  ],
  "author": "Rich Harris",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sveltejs/svelte-todomvc/issues"
  },
  "homepage": "https://github.com/sveltejs/svelte-todomvc",
  "devDependencies": {
    "svelte": "^3.31.0",
    "svelte-loader": "^2.13.6",
    "webpack": "^5.10.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
